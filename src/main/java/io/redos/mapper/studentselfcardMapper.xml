<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="io.redos.mapper.StudentSelfCardMapper">
    <resultMap id="studentCard" type="io.redos.entity.StudentSelfcardBean">
        <id property="id" column="id"/>
        <result  property="studentId" column="student_id"/>
        <result  property="native_" column="native"/>
        <result  property="issueDate" column="issue_date"/>
        <result  property="endDate" column="end_date"/>
        <result  property="note" column="note"/>
    </resultMap>

    <select id="selectCardById" parameterType="long" resultMap="studentCard">
        select id,student_id,native,issue_Date,end_date,note from t_student_selfcard where student_id=#{id}
    </select>
</mapper>